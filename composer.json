{
    "name": "jarjak/dbal-manager",
    "description" : "Helper class for Doctrine DBAL",
    "keywords": ["dbal", "database", "persistence", "queryobject"],
    "homepage": "https://github.com/JarJak/DBALManager",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Jarek Jakubowski",
            "email": "egger1991@gmail.com"
        }
    ],
    "require": {
        "doctrine/dbal": "~2.4"
    },
    "require-dev": {
        "symfony/var-dumper": "~2.6"
    },
    "suggest": {
        "symfony/var-dumper": "DBALManager uses VarDumper to nicely dump SQL queries"
    },
    "autoload": {
        "psr-0": { "JarJak": "src" }
    }
}
